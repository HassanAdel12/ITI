@model IEnumerable<Student>

@{
}


<p>
    <a asp-action="Create" asp-controller="Student">"Create New"</a>
</p>

<img src="~/images/image.png" class="img-fluid" />

<h2>Student</h2>

<table class="table">
    <tr>
        <th>
            Id
@*             <label asp-for="FirstOrDefault().Id"></label>
 *@        </th>
        <th>
            Full Name
@*             <label asp-for="FirstOrDefault().FullName"></label>
 *@        </th>
        <th>
            Graduated
@*             <label asp-for="FirstOrDefault().Graduated"></label>
 *@        </th>
        <th>
            Gpa
@*             <label asp-for="FirstOrDefault().Gpa"></label>
 *@        </th>
        <th>
            City
@*             <label asp-for="FirstOrDefault().City.Name"></label>
 *@        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>
                @item.Id
            </td>
            <td>
                @item.FullName
            </td>
            <td>
                @item.Graduated
            </td>
            <td>
                @item.Gpa
            </td>
            <td>
                @item.City.Name
            </td>
            <td>
                <a asp-action="Edit" asp-controller="Student" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Delete" asp-controller="Student" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
    }
</table>